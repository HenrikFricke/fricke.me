//- Sort project list alphabetical
- opensource.projects.sort(function (a,b) {
  - if (a.name < b.name)
    - return -1
  - if (a.name > b.name)
    - return 1
  - return 0
- })

.open-source
  .open-source__container
    .open-source__title #{opensource.title}
    .open-source__description #{opensource.description}
    .open-source__projects
      ul.open-source__list
        each item in opensource.projects
          li.open-source__item
            a.open-source__link(href="#{item.url}", target="_blank") #{item.name}
    .open-source__github
      span.open-source__github-note #{opensource.github.note}
      a(href="https://github.com/HenrikFricke/fricke.me", target="_blank").open-source__github-button #{opensource.github.button}
